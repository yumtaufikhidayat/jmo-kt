<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.fragment.HomeDetailFragment">

    <include
        android:id="@+id/toolbarHomeDetail"
        layout="@layout/toolbar_main_general"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvServiceProgramDetail"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginHorizontal="16dp"
        tools:listitem="@layout/item_service_program"
        app:layout_constraintTop_toBottomOf="@id/toolbarHomeDetail"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>